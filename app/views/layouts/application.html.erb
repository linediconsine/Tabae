<body>

    <nav class="navbar navbar-expand-md navbar-dark bg-dark px-1">
        <%= link_to "TaBaE", root_path , class: "navbar-brand"%>

        <span class="offline_message text-light">Offline</span>
        <% if action_name != 'coming_soon' %>
        <% if user_signed_in?  %>

          <%= render "application/breadcrumbs" %>

          <div style="margin-left:auto" class="dropdown">
            <button type="button" class="btn btn-secondary dropdown-toggle " data-bs-toggle="dropdown" aria-expanded="false">
              <span class="d-none d-sm-block" style="float:left">Hi, <%= current_user.email %></span>
            </button>
            <ul class="dropdown-menu dropdown-menu-end dropdown-menu-lg-start">
              <% if @edit_mode %>
                <%= link_to t("edit"), request.query_parameters.merge({:edit => false}), class: "dropdown-item" %>
              <% else %>
                <%= link_to t("edit"), request.query_parameters.merge({:edit => true}), class: "dropdown-item" %>
              <% end %>

              <%= link_to  t("profile"), edit_user_registration_path , class: "dropdown-item" %>

              <%= link_to  t("logout"), destroy_user_session_path, method: :delete , class: "dropdown-item" %>
            </ul>
          </div>



                  <% else %>

                    <%= link_to "Sign up", new_user_registration_path, :class => 'nav-link text-white',:style => 'margin-left: auto;'  %>
                    <%= link_to "Login", new_user_session_path, :class => 'btn btn-secondary'  %>
                  
                  <% end %>
              <% end %>
         
    </nav>
    <%= render "notice" %>
    
    <section role="main" class="container-fluid wrapper mt-2 <%=@body_class%>" >
        <%= yield %>
    </section>


    <%= javascript_pack_tag 'checkconnection' %>
 

